{
  "multipart": [
    {
      "apply": { "model": "industrial:block/copper_wire_center" }
    },
    {
      "when": { "north": true },
      "apply": { "model": "industrial:block/copper_wire_north" }
    },
    {
      "when": { "east": true },
      "apply": { "model": "industrial:block/copper_wire_east" }
    },
    {
      "when": { "south": true },
      "apply": { "model": "industrial:block/copper_wire_south" }
    },
    {
      "when": { "west": true },
      "apply": { "model": "industrial:block/copper_wire_west" }
    },
    {
      "when": { "up": true },
      "apply": { "model": "industrial:block/copper_wire_up" }
    },
    {
      "when": { "down": true },
      "apply": { "model": "industrial:block/copper_wire_down" }
    }
  ]
}